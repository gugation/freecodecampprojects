function palindrome(str) {
  str = str.split(/[\W^_]/g).join("").toLowerCase();
  console.log(str);
  var array=[];
  for (var i = 0; i< str.length;i++){  
    array.push(str[i]);
  }
  while(array.length >= 2){
    if(array.shift() != array.pop()){
      return false;
    }
  }
  return true;
}

console.log(palindrome("0_0 (: /-\ :) 0-0"));
